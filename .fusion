{
  "engine": "scss",
  "publish-template": "@no-repeat/theme-template",
  "prefix": ".vext", 
  "import": {
    "sources": {
      "dir": "lib",
      "entry-file": {
        "sass": "lib/index.scss"
      },
      "entries": {}
    },
    "demos": {
      "dir": "demos",
      "entry-file": {
        "html": "index.html",
        "deps": "deps.json"
      },
      "entries": {}
    }
  },
  "variables": {
    "color": "lib/core/lib/style/_color.scss",
    "corner": "lib/core/lib/style/_corner.scss",
    "font": "lib/core/lib/style/_font.scss",
    "icon": "lib/core/lib/style/_icon.scss",
    "line": "lib/core/lib/style/_line.scss",
    "shadow": "lib/core/lib/style/_shadow.scss",
    "size": "lib/core/lib/style/_size.scss",
    "button": "lib/button/lib/scss/variable.scss"
  }
}